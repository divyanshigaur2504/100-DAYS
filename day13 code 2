/*Write a program to print numbers from 1 to n*/



#include <stdio.h>

int main()
{
    int n, i;

    // Taking input for n
    printf("Enter a positive integer: ");
    scanf("%d", &n);

    // Loop to print numbers from 1 to n
    for (i = 1; i <= n; i++) {
        printf("%d ", i);
    }

    printf("\n");

    return 0;
}

